{
	"name": "Turbo Colors",
	"type": "dark",
	"colors": {		
		"activityBar.foreground": "#ffffff",
		"activityBar.background": "#000000",
		"editor.background": "#0b012e",
		"editor.foreground": "#fffeee",
		"titleBar.activeBackground": "#0000dd",
		"titleBar.activeForeground": "#ffffff",
		"activityBarBadge.background": "#007acc",
		"sideBarTitle.foreground": "#ffffff",
		"sideBar.background": "#1f1f1f",
		"sideBar.foreground": "#e8ed00",
		"sideBarSectionHeader.foreground": "#ffff00",
		"list.activeSelectionBackground": "#555555",
		"list.activeSelectionForeground": "#ffffff",
		"list.inactiveSelectionBackground": "#3f3f3f",
		"list.inactiveSelectionForeground": "#ffffff",
		"statusBar.background": "#000080"
		
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#09f7ff"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder",				
				"punctuation.definition.variable.php",
				"source.python"
			],
			"settings": {
				"foreground": "#F8DC3D"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#fd2b2b"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"keyword.control",
				"storage.type",
				"storage.modifier",
				"punctuation.section.embedded"
			],
			"settings": {
				"foreground": "#ffffff",
				"fontStyle": "bold"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
